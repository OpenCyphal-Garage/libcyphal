{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/linux/example/media_on_linux",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/build",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "cortex-debug",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/build_ci_ontarget_s32k/test_util_math.elf",
            "name": "Debug tests",
            "request": "launch",
            "servertype": "jlink",
            "device": "S32K148",
            "svdFile": "${workspaceRoot}/test/ontarget/S32K148EVB/S32K148.svd"
        },
        {
            "type": "lldb",
            "name": "(lldb) Launch",
            "request": "launch",
            "program": "${workspaceFolder}/build/tests/test_util_math",
            "args": ["--gtest_filter=SaturatedMathTest/* MixedTypeMathParams<unsigned, signed>.SaturatingSubMaxMax"],
            "cwd": "${workspaceFolder}"
        }
    ]
}
