{
    "spellright.ignoreRegExps": [
        "/DLCs/g",
        "/Kirienko/g",
        "/Pavel/g",
        "/gtests/g",
        "/(lib)?cyphal/g",
        "/ovfl/g"
    ],
    "files.associations": {
        "array": "cpp",
        "*.tcc": "cpp",
        "iosfwd": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "initializer_list": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "optional": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "type_traits": "cpp",
        "tuple": "cpp",
        "typeinfo": "cpp",
        "utility": "cpp",
        "algorithm": "cpp",
        "*.jinja": "jinja",
        "deque": "cpp",
        "new": "cpp",
        "chrono": "cpp",
        "cinttypes": "cpp",
        "cstddef": "cpp",
        "ctime": "cpp",
        "functional": "cpp",
        "iomanip": "cpp",
        "ratio": "cpp",
        "thread": "cpp",
        "__bit_reference": "cpp",
        "__config": "cpp",
        "__debug": "cpp",
        "__errc": "cpp",
        "__functional_base": "cpp",
        "__hash_table": "cpp",
        "__locale": "cpp",
        "__mutex_base": "cpp",
        "__node_handle": "cpp",
        "__nullptr": "cpp",
        "__split_buffer": "cpp",
        "__string": "cpp",
        "__threading_support": "cpp",
        "__tree": "cpp",
        "__tuple": "cpp",
        "atomic": "cpp",
        "bitset": "cpp",
        "cstdarg": "cpp",
        "ios": "cpp",
        "iterator": "cpp",
        "locale": "cpp",
        "map": "cpp",
        "mutex": "cpp",
        "set": "cpp",
        "string": "cpp"
    },
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.configureOnOpen": false,
    "cmake.buildDirectory": "${workspaceFolder}/test/build_vscode/${buildKit}/${buildType}/build",
    "cmake.sourceDirectory": "${workspaceFolder}/test",
    "cmake.buildBeforeRun": true,
    "cmake.copyCompileCommands": "${workspaceFolder}/.vscode/compile_commands.json",
    "cmake.generator": "Ninja",
    "cmake.configureArgs": [
        "-DLIBCYPHAL_EXT_FOLDER=${workspaceFolder}/test/build_vscode/${buildKit}/${buildType}/ext"
    ],
    "editor.wordWrapColumn": 120,
    "editor.rulers": [
        120
    ],
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "cpp"
    ],
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "verification/build_vscode/**/suite/{test,Test,TEST}_*",
            "cwd": "${workspaceFolder}"
        }
    ],
    "testMate.cpp.debug.configTemplate": {
        "type": "cppdbg",
        "linux": {
            "MIMode": "gdb"
        },
        "osx": {
            "MIMode": "lldb",
            "externalConsole": false
        },
        "windows": {
            "type": "cppvsdbg"
        },
        "program": "${exec}",
        "args": "${argsArray}",
        "cwd": "${workspaceFolder}",
        "env": "",
        "environment": "",
        "sourceFileMap": "${sourceFileMapObj}"
    },
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=120"
    ]
}
